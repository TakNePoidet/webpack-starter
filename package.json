	"scripts": {
		"start": "webpack serve --config ./build/dev.webpack.config.js",
		"build": "webpack --mode=production --config ./build/prod.webpack.config.js",
		"analyzer": "webpack --mode=production --config ./build/analyzer.webpack.config.js",
		"lint": "eslint --fix .",
		"svg-sprite": "mkdir -p ./src/images/ && svg-spreact --optimize true --tidy false ./src/images/icon/ > ./src/images/sprite.svg",
		"stylelint": "stylelint --fix \"src/**/*.{scss,css,sass}\"",
		"prettier": "prettier --ignore-unknown --write .",
		"htmlhint": "htmlhint \"src/templates/*\"",
		"beautifier": "yarn htmlhint && yarn lint && yarn prettier",
		"deploy": "yarn build && gh-pages -d dist"
